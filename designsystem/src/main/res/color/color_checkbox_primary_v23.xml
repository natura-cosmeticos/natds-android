<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:state_enabled="false" android:state_checked="true"
        android:color="?colorMediumEmphasis"
        android:alpha="?opacity06"/>

    <item
        android:state_enabled="false" app:state_indeterminate="true"
        android:color="?colorMediumEmphasis"
        android:alpha="?opacity06"/>
    <item
        android:state_checked="true"
        android:color="?colorPrimary"/>
    <item
        app:state_indeterminate="true"
        android:color="?colorPrimary"/>
    <item
        android:state_selected="true"
        android:color="?colorPrimary"/>
    <item
        android:state_activated="true"
        android:color="?colorPrimary"/>
    <item
        android:state_enabled="false"
        android:color="?colorMediumEmphasis"
        android:alpha="?opacity06" />

    <item android:color="?colorMediumEmphasis"/>
</selector>