<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="ds_menu">
        <attr name="menu_label" format="string" />
        <attr name="menu_label_color" format="reference" />
        <attr name="menu_icon" format="reference" />
        <attr name="menu_label_size" format="reference" />
        <attr name="menu_selected_drawable" format="reference" />
        <attr name="menu_opened_drawable" format="reference" />
        <attr name="menu_is_selected" format="boolean" />
        <attr name="menu_is_enabled" format="boolean" />
        <attr name="menu_is_opened" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ds_submenu">
        <attr name="submenu_label" format="string" />
        <attr name="submenu_label_color" format="reference" />
        <attr name="submenu_label_size" format="reference" />
        <attr name="submenu_selected_drawable" format="reference" />
        <attr name="submenu_is_selected" format="boolean" />
        <attr name="submenu_is_enabled" format="boolean" />
    </declare-styleable>

</resources>
